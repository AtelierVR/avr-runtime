                    <ui:Button text="Register" name="avr-register-submit" class="avr background right left bottom input box" />
                </ui:Tab>
            </ui:TabView>
            <ui:VisualElement name="Footer" style="flex-grow: 1;" />
        </ui:VisualElement>
    </ui:VisualElement>
